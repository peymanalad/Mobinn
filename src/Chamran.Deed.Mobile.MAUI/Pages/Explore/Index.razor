    private async ValueTask<List<ExploreDataModel>> LoadPostCategories()//ItemsProviderRequest request
    {
        try
        {
            //_filter.MaxResultCount = request.Count;
            //_filter.SkipCount = request.StartIndex;

            await UserDialogsService.Block();

            await WebRequestExecuter.Execute(
            async () => await ExploreDataService.GetExploreDataAsync(),
            async (result) =>
            {

                posts = result;


            }
            );
            await WebRequestExecuter.Execute(
            async () => await PostsDataService.GetPostsDataAsync(0),
            async (result) =>
            {

                foreach (var exploreDataModel in result)
                {
                    posts.Add(new ExploreDataModel()
                        {
                            PostGroupDescription = "",
                            ImageFileId = exploreDataModel.PostFile,
                            Id = exploreDataModel.CategoryId
                        });
                }

                await UserDialogsService.UnBlock();
                StateHasChanged(); // not always necessary... see link below

            }
            );

            return posts;
        }
        catch (Exception e)
        {
            System.Console.WriteLine(e);
        }
        await UserDialogsService.UnBlock();
        StateHasChanged(); // not always necessary... see link below

        return posts;

    }

    protected override async void OnInitialized()
    {
        await LoadPostCategories();
        await SyncExplore();


        await base.OnInitializedAsync();
    }

    private async Task SyncExplore()
    {
        try
        {
            await WebRequestExecuter.Execute(
            async () => await _postsAppService.GetPostCategoriesForView(),
            async (result) =>
            {
                //posts = new ItemsProviderResult<GetPostCategoriesForViewDto>(result.Items, result.TotalCount);
                foreach (var row in result.Items)
                {
                    var imgSource = await DownloadPhoto(row.Base64Image);
                    if (!string.IsNullOrEmpty(imgSource))
                    {
                        await ExploreDataService.CreateOrUpdateExploreDataAsync(new ExploreDataModel()
                            {
                                ImageFileId = row.Base64Image,
                                PostGroupDescription = row.PostGroupDescription,
                                Id = row.Id
                            });
                    }

                }
                await LoadPostCategories();

            }
            );

        }
        catch (Exception e)
        {
            System.Console.WriteLine(e);
            throw;
        }
    }

    private async Task<string> DownloadPhoto(Guid? fileId)
    {
        try
        {

            var path = Path.Combine(FileSystem.CacheDirectory, fileId.ToString());
            if (File.Exists(path)) return path;
            var data = await _fileProviderAppService.GetImageSourceBase64(fileId.ToString());
            await File.WriteAllTextAsync(path, data);
            return path;

        }
        catch (Exception e)
        {
            Console.WriteLine(e);
        }
        return "";
    }

    private void NavigateTo(GetPostCategoriesForViewDto post)
    {
        _navigationService.NavigateTo(NavigationUrlConsts.AllPosts);
    }
    private string GetImageSource(Guid? fileId)
    {
        try
        {
            var path = Path.Combine(FileSystem.CacheDirectory, fileId.ToString());
            var res = _fileProviderAppService.GetLocalImageSourceBase64(path).Result;
            if (!res.EndsWith("=\""))
            {
                res = "\"\""; ;
                if (File.Exists(path))
                    File.Delete(path);
            }
            return res;
        }
        catch (Exception e)
        {
            Console.WriteLine(e);
            return "";
        }
        //return @"data:image/png;base64,";

    }
